<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Patients Data</title>
  <style>
    table {
      width: 100%;
      border-collapse: collapse;
    }
    th, td {
      padding: 10px;
      text-align: left;
      border: 1px solid #ddd;
    }
    th {
      background-color: #f2f2f2;
    }
  </style>
</head>
<body>
  <h1>Patients Data</h1>
  <table>
    <thead>
      <tr>
        <th> Patient ID</th>
        <th>First Name</th>
      </tr>
    </thead>
    <tbody>
      <% result.forEach( row => {%>
        <tr>
          <td><%= row.patient_id %></td>
          <td><%= row.first_name %></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
</body>
</html>
